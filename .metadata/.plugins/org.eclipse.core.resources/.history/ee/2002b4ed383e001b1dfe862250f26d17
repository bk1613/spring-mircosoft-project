package com.synex.controller;

public class DecodeWays {
	
	public int decode(String in) {
		
		int length = in.length();
		int[] dp = new int[length + 1];
		
		if(in.charAt(0) == '0') {
			return 0;
		}
		
		for(int i = 1; i < in.length(); i++) {
			char c = in.charAt(i);
			char p = in.charAt(i-1);
			
			if (c == '0' && (p == '0' || p > '2')) {
	            return 0;
	        }
			
			if(p == '0') {
				dp[i + 1] = dp[i];
			}else if(p == '1') {
				if (c == '0') {
	                dp[i + 1] = dp[i - 1];
	            } else {
	                dp[i + 1] = dp[i - 1] + dp[i];
	            }
			}else if(p == '2') {
				if (c == '0') {
	                dp[i + 1] = dp[i - 1];
	            } else if (c <= '6') {
	                dp[i + 1] = dp[i] + dp[i - 1];
	            } else {
	                dp[i + 1] = dp[i];
	            }
			}else {
				dp[i + 1] = dp[i];
			}
			
			
		}
		
		return dp.length;
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String in = "12";
		
		
		System.out.println(new DecodeWays().decode(in));
	}

}
